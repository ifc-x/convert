{
  "name": "@ifc-x/convert",
  "version": "0.1.0",
  "description": "Convert IFC/FRAG files to FRAG/SQLite in Node.js, browser and CLI.",
  "author": "Lauri Tunnela <lauri@tunne.la>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./assets/*": "./dist/assets/*"
  },
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "files": [
    "dist"
  ],
  "bin": {
    "ifc-x-convert": "bin/cli.js"
  },
  "scripts": {
    "build": "vite build",
    "examples": "vite --config vite.config.examples.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "IFC",
    "FRAG",
    "SQLite",
    "converter",
    "conversion",
    "Node.js",
    "browser",
    "CLI",
    "BIM",
    "library"
  ],
  "dependencies": {
    "@thatopen/fragments": "^3.1.7",
    "sql.js": "^1.13.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "web-ifc": "^0.0.71"
  },
  "devDependencies": {
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  }
}
