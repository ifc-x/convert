{
  "name": "@ifc-x/convert",
  "version": "0.1.2",
  "description": "Convert IFC/FRAG files to FRAG/SQLite in Node.js, browser and CLI.",
  "author": "Lauri Tunnela <lauri@tunne.la>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.node.cjs",
  "module": "./dist/index.node.js",
  "browser": "./dist/index.browser.js",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "exports": {
    ".": {
      "import": {
        "browser": "./dist/index.browser.js",
        "default": "./dist/index.node.js"
      },
      "require": "./dist/index.node.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "ifc-x-convert": "bin/cli.js"
  },
  "scripts": {
    "build": "vite build --config vite.config.browser.js && vite build --config vite.config.node.js",
    "build:node": "vite build --config vite.config.node.js",
    "build:browser": "vite build --config vite.config.browser.js",
    "examples": "vite --config vite.config.examples.js",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "vitest run --environment node",
    "test:node:watch": "vitest --environment node --watch",
    "test:browser": "vitest run --browser",
    "test:browser:watch": "vitest --browser --watch",
    "prepare": "husky",
    "preversion": "npm run build && npm run test && git add -f dist",
    "version": "git commit -m 'chore(build): update dist for version %s' || true",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "IFC",
    "FRAG",
    "SQLite",
    "converter",
    "conversion",
    "node",
    "browser",
    "CLI",
    "BIM",
    "library"
  ],
  "dependencies": {
    "@thatopen/fragments": "^3.1.7",
    "sql.js": "^1.13.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "web-ifc": "^0.0.71"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@vitest/browser": "^3.2.4",
    "husky": "^9.1.7",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  }
}
